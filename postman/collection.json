{
  "info": {
    "name": "Wave1-Pools",
    "_postman_id": "e2a1d77b-7e2f-4c3e-9b7a-20250925",
    "description": "Wave-1 DEX Pool discovery examples for Orca, Meteora, PumpSwap.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Orca - Whirlpool List (CLMM)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "https://api.orca.so/v1/whirlpool/list",
          "protocol": "https",
          "host": ["api", "orca", "so"],
          "path": ["v1", "whirlpool", "list"]
        },
        "description": "List Orca Whirlpools. Returns a large list; treat as a paginable feed operationally."
      }
    },
    {
      "name": "Orca - Whirlpool Details (by address) [UNSTABLE]",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "https://api.orca.so/v1/whirlpool/{{whirlpool_pubkey}}",
          "protocol": "https",
          "host": ["api", "orca", "so"],
          "path": ["v1", "whirlpool", "{{whirlpool_pubkey}}"]
        },
        "description": "May return diagnostic wrapper (e.g., Cloudflare 1016) instead of pool JSON depending on region/provider. Prefer the list endpoint or SDK/on-chain for per-pool reads."
      }
    },
    {
      "name": "Meteora - DLMM Pairs (paged)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "https://dlmm-api.meteora.ag/pair/all_with_pagination?page={{meteora_page}}&limit={{meteora_limit}}",
          "protocol": "https",
          "host": ["dlmm-api", "meteora", "ag"],
          "path": ["pair", "all_with_pagination"],
          "query": [
            { "key": "page", "value": "{{meteora_page}}" },
            { "key": "limit", "value": "{{meteora_limit}}" }
          ]
        },
        "description": "List DLMM pairs (paged). Use page/limit and iterate."
      }
    },
    {
      "name": "Meteora - DLMM Pair Details (by address)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "https://dlmm-api.meteora.ag/pair/{{dlmm_pair_address}}",
          "protocol": "https",
          "host": ["dlmm-api", "meteora", "ag"],
          "path": ["pair", "{{dlmm_pair_address}}"]
        },
        "description": "Get a DLMM pair by address."
      }
    },
    {
      "name": "DexScreener - Search (PumpSwap shards)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "https://api.dexscreener.com/latest/dex/search?q={{pumpswap_query}}",
          "protocol": "https",
          "host": ["api", "dexscreener", "com"],
          "path": ["latest", "dex", "search"],
          "query": [{ "key": "q", "value": "{{pumpswap_query}}" }]
        },
        "description": "Shard queries (e.g., 'pumpswap a', 'pumpswap 1') and dedupe by pairAddress."
      }
    },
    {
      "name": "Birdeye - Pair Overview (enrichment)",
      "request": {
        "method": "GET",
        "header": [
          { "key": "X-API-KEY", "value": "{{birdeye_api_key}}" },
          { "key": "x-chain", "value": "solana" },
          { "key": "Accept", "value": "application/json" }
        ],
        "url": {
          "raw": "https://public-api.birdeye.so/defi/v3/pair/overview/single?address={{pair_address}}",
          "protocol": "https",
          "host": ["public-api", "birdeye", "so"],
          "path": ["defi", "v3", "pair", "overview", "single"],
          "query": [{ "key": "address", "value": "{{pair_address}}" }]
        },
        "description": "Enrich a discovered pair with liquidity, volume24h, feeBps, created_at."
      }
    }
  ],
  "variable": [
    { "key": "whirlpool_pubkey", "value": "" },
    { "key": "meteora_page", "value": "0" },
    { "key": "meteora_limit", "value": "200" },
    { "key": "dlmm_pair_address", "value": "" },
    { "key": "pumpswap_query", "value": "pumpswap a" },
    { "key": "birdeye_api_key", "value": "" },
    { "key": "pair_address", "value": "" }
  ]
}
